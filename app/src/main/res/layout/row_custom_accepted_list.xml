<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/view_foreground"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:padding="@dimen/_5sdp">

        <TextView
            android:id="@+id/ItemCode"
            android:layout_width="226dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="30dp"
            android:text="0115680592"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="16dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tvDesc"
            android:layout_width="204dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ItemCode"
            android:layout_alignStart="@+id/ItemCode"
            android:text="SAME"
            android:textAllCaps="true"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tvType"
            android:layout_width="165dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="2dp"
            android:fontFamily="sans-serif"
            android:foregroundGravity="center"
            android:gravity="center"
            android:padding="@dimen/_5sdp"
            android:text="9/17/2018 2:12:06 PM"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/desc1"
            android:layout_width="207dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_esn_imei"
            android:layout_alignStart="@+id/ItemCode"
            android:layout_marginStart="0dp"
            android:text="MULLERIYAWA"
            android:textAlignment="viewStart"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/time"
            android:visibility="gone"
            android:layout_width="207dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/desc1"
            android:layout_alignStart="@+id/ItemCode"
            android:layout_marginStart="0dp"
            android:text="0 hrs 0 min"
            android:textAlignment="viewStart"
            android:textSize="14sp" />

        <!--<TextView-->
            <!--android:id="@+id/desc1"-->
            <!--android:layout_width="207dp"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_below="@+id/tvDesc"-->
            <!--android:layout_alignStart="@+id/ItemCode"-->
            <!--android:layout_marginStart="0dp"-->
            <!--android:text="MULLERIYAWA"-->
            <!--android:textAlignment="viewStart"-->
            <!--android:textSize="14sp" />-->

        <TextView
            android:id="@+id/tv_esn_imei"
            android:layout_width="207dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tvDesc"
            android:layout_alignStart="@+id/ItemCode"
            android:layout_marginStart="0dp"
            android:text="tv_esn_imei"
            android:textAlignment="viewStart"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tvMore"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:textAlignment="textEnd"
            android:layout_alignEnd="@+id/tvType"
            android:layout_below="@+id/desc1"
            android:text="More... "
            android:textSize="18sp"
            android:visibility="visible"
            android:textColor="@color/gray_btn_bg_pressed_color"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />
        <TextView
            android:id="@+id/seq_no"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical"
            android:text="051"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold" />


        <Button

            android:id="@+id/btnCheck"
            android:layout_width="122dp"
            android:layout_height="42dp"
            android:layout_below="@+id/tvType"
            android:layout_alignBottom="@+id/tvType"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_marginBottom="-44dp"
            android:background="@drawable/rounded_background_primary"
            android:fontFamily="sans-serif"
            android:text="Materials"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="@dimen/_12sdp"
            android:textStyle="normal|bold" />
    </RelativeLayout>

